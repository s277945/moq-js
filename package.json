{
	"name": "moq-js",
	"type": "module",
	"private": true,
	"workspaces": [
		"lib",
		"logger",
		"web"
	],
	"scripts": {
		"start": "npm run dev --workspace=web",
		"dev": "concurrently \"npm run dev --workspace=logger\" \"npm run dev --workspace=web\"",
		"build": "npm run build --workspaces",
		"lint": "npm run lint --workspaces",
		"fmt": "npm run fmt --workspaces"
	},
	"dependencies": {
		"concurrently": "^8.2.2"
	},
	"devDependencies": {
		"@types/node": "^20.10.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.2"
	}
}
